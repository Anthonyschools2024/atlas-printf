#include <stdarg.h>
#include <stdio.h>

int _printf(const char *format,...) {
    va_list args;
    va_start(args, format);

    int count = 0; // To keep track of the number of characters printed

    for (const char *p = format; *p!= '\0'; p++) {
        if (*p == '%') {
            p++; // Move past the '%' character
            switch (*p) {
                case 'c': {
                    char ch = va_arg(args, int); // char argument passed as int
                    putchar(ch);
                    count++;
                    break;
                }
                case 's': {
                    const char *str = va_arg(args, const char *);
                    while (*str && *str!= ' ') { // Stop at space or null terminator
                        putchar(*str++);
                        count++;
                    }
                    break;
                }
                case '%': {
                    putchar('%');
                    count++;
                    break;
                }
                default:
                    break; // Ignore unknown specifiers
            }
        } else {
            putchar(*p);
            count++;
        }
    }

    va_end(args);
    return count;
}

int main() {
    int num = 42;
    char name[] = "Alice";
    printf("Number: %d, Name: %s\n", num, name);
    _printf("Character: %c, String: %s, Percent: %%\n", 'A', name, "%");
    return 0;
}
