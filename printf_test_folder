int _printf(const char *format,...) {
    va_list args;
    int count = 0;

    // Initialize the argument list
    va_start(args, format);

    // Iterate over the format string
    for (const char *ptr = format; *ptr!= '\0'; ptr++) {
        // Check for conversion specifier
        if (*ptr == '%') {
            ptr++; // Move past the '%' character

            // Handle % character
            if (*ptr == '%') {
                write(1, "%", 1); // Write the '%' character
                count++;
            }
            // Handle 'c' character
            else if (*ptr == 'c') {
                char ch = (char)va_arg(args, int); // Cast to char since va_arg returns int
                write(1, &ch, 1); // Write the character
                count++;
            }
            // Handle 's' character
            else if (*ptr == 's') {
                const char *str = va_arg(args, const char *);
                for (; *str; str++, count++) {
                    write(1, str, 1); // Write each character of the string
                }
            }
        } else {
            write(1, ptr, 1); // Write non-specifier characters directly
            count++;
        }
    }

    // Clean up the argument list
    va_end(args);

    return count;
}

